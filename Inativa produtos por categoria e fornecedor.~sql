SELECT DISTINCT a.seqproduto, a.nroempresa, b.desccompleta
 FROM consinco.mrl_produtoempresa a 
       JOIN consinco.map_produto b
            ON a.seqproduto = b.seqproduto
       JOIN consinco.map_familia c
            ON c.seqfamilia = b.seqfamilia
       JOIN consinco.map_famfornec d
            ON c.seqfamilia = d.seqfamilia
       JOIN consinco.map_famdivcateg e
            ON c.seqfamilia = e.seqfamilia
       JOIN consinco.map_categoria f
            ON e.seqcategoria = f.seqcategoria
       where a.nroempresa = 1 and 
             a.statuscompra = 'A' and
             f.statuscategor = 'A' and
             e.status = 'A' and
             f.nivelhierarquia = 3 and
             f.caminhocompleto LIKE 'PERFUMARIA%' and
             --criar subquery para excluir produtos que não aparecem para dag ou ibiapina
             d.seqfornecedor not in (1161, 1156) 
             
             ORDER BY 3
             ;

